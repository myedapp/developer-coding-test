{
  "name": "coding-test",
  "version": "0.0.1",
  "description": "coding-test",
  "keywords": [],
  "author": "Chris Kelly",
  "license": "UNLICENSED",
  "jest": {
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json"
    ],
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "modulePaths": [
      "<rootDir>",
      "<rootDir>/src",
      "<rootDir>/src/assets",
      "<rootDir>/src/components"
    ],
    "moduleNameMapper": {
      "\\.(svg|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|svg)$": "<rootDir>/mocks/fileMock.js",
      "\\.(css|scss)$": "<rootDir>/mocks/styleMock.js"
    },
    "coverageDirectory": "./coverage"
  },
  "scripts": {
    "setup": "npm --no-progress --production=false install",
    "eslint": "cross-env eslint ./src/ --ext .jsx --ext .js",
    "dev": "cross-env BABEL_ENV=webpack webpack-dev-server --content-base build/",
    "build": "cross-env BABEL_ENV=webpack webpack --progress",
    "test": "cross-env BABEL_ENV=test jest",
    "test-watch": "cross-env BABEL_ENV=test jest --watch",
    "cover": "cross-env BABEL_ENV=test jest --coverage"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "eslint": "^4.8.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss": "^6.0.13",
    "postcss-loader": "^2.0.6",
    "react-test-renderer": "^16.0.0",
    "regenerator-runtime": "^0.11.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-react-loader": "^0.4.5",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-circular-progressbar": "^0.2.2",
    "react-dom": "^16.0.0",
    "sass-rem": "^2.0.0"
  }
}
